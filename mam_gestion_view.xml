<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>


<!--contact-->
	<record model="ir.ui.view" id="view_contact_tree">
		<field name="name">contact.tree</field>
		<field name="model">mam.contact</field>
		<field name="arch" type="xml">
			<tree string="contact">
				<field name="nom"/>
				<field name="prenom"/>
				<field name="adresse"/>
				<field name="tel_fixe"/>
				<field name="tel_mobile"/>
				<field name="enfant_ids"/>
			</tree>
		</field>
	</record>
	<record model="ir.ui.view" id="view_contact_form">
		<field name="name">contact.form</field>
		<field name="model">mam.contact</field>
		<field name="arch" type="xml">
			<form string="contact">
				<field name="nom"/>
				<field name="prenom"/>
				<field name="adresse"/>
				<field name="tel_fixe"/>
				<field name="tel_mobile"/>
<!--				<field name="enfant_ids"/>-->
			</form>
		</field>
	</record>


	<record model="ir.actions.act_window" id="action_am">
		<field name="name">Assistante maternelle</field>
		<field name="res_model">mam.contact</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>

	<menuitem name="MAM" id="menu_top_mam" action="action_am"/>
	<menuitem name="MAM" id="menu_rub_mam" parent="menu_top_mam" action="action_am"/>
	<menuitem name="Ass. Mat." id="menu_am_item" parent="menu_rub_mam" action="action_am"/>

</data>
</openerp>
